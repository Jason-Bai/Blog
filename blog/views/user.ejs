<%- include header %>
<% if(posts) { posts.forEach(function (post, index) { %>
     <p><h2><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title  %></a></h2></p>
     <p class="info">
         Author : <a href="/u/<%= post.name %>"><%= post.name %></a> |
         Date : <%= post.time.minute %> |
         Tags :
         <% if(post.tags) { post.tags.forEach(function (tag, index) { %>
             <a class="tag" href="/tags/<%= tag %>"><%= tag %></a>
         <% })} %>
     </p>
     <%- post.post %>
     <p class="info">Access Times : <%= post.pv ? post.pv : 0 %> | Comment Times : <%= post.comments.length == 0 ? 0 : post.comments.length %></p>
<% })} %>
<%- include paging %>
<%- include footer %>
